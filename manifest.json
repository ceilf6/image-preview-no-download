{
    "manifest_version": 3,
    "name": "图片预览器",
    "version": "1.0",
    "description": "阻止图片链接自动下载，改为在新窗口预览",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "action": {
        "default_popup": "popup.html",
        "default_title": "图片预览器"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}